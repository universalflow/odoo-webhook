<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_model_form_inherited" model="ir.ui.view">
        <field name="name">ir.model.form.inherited</field>
        <field name="model">ir.model</field>
        <field name="inherit_id" ref="base.view_model_form"/>
        <field name="arch" type="xml">
            <page name="views" position="after">
                <page name="restapi" string="Restapi Schema">
                    <group name="schema" string="Schema">
                        <group>
                            <field name="fields_schema" />
                        </group>
                        <group>
                            <field name="fields_post_Schema" />
                        </group>
                    </group>
                </page>
            </page>
        </field>
    </record>

</odoo>
